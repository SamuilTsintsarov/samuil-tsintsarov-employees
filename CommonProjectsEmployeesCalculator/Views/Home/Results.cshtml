@model IEnumerable<BusinessLogicLayer.Models.CommonProject>

@{
    ViewData["Title"] = "Results";
}

<h2>Common Projects</h2>

<table class="table">
    <thead>
        <tr>
            <th>Employee ID #1</th>
            <th>Employee ID #2</th>
            <th>Project ID</th>
            <th>Days Worked</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var project in Model)
        {
            <tr>
                <td>@project.EmployeeId1</td>
                <td>@project.EmployeeId2</td>
                <td>@project.ProjectID</td>
                <td>@project.DaysWorked</td>
            </tr>
        }
    </tbody>
</table>